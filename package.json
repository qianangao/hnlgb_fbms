{
  "name": "hnlgb_fbms",
  "version": "1.0.0",
  "private": true,
  "description": "Hainan Province Retired cadres Service management platform",
  "scripts": {
    "start": "cross-env MOCK=none umi dev",
    "mock": "umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "ui": "umi ui",
    "build": "umi build",
    "build:prod": "cross-env ANALYZE=1 umi build",
    "deploy": "npm run site && npm run gh-pages",
    "fetch:blocks": "pro fetch-blocks && npm run prettier",
    "format-imports": "cross-env import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\"",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.less": "stylelint --syntax less",
    "src/**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "src/**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/compatible": "^0.0.1-rc.0",
    "@ant-design/icons": "^4.0.2",
    "@ant-design/pro-layout": "^5.0.4",
    "@ant-design/pro-table": "^2.1.7",
    "@antv/data-set": "^0.10.2",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/react-router": "^5.0.2",
    "antd": "^4.4.2",
    "bizcharts": "^3.5.8",
    "bizcharts-plugin-slider": "~2.1.1-beta.1",
    "classnames": "^2.2.6",
    "crypto-js": "^4.0.0",
    "gg-editor": "^2.0.2",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.11",
    "lodash-decorators": "^6.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "nzh": "^1.0.3",
    "omit.js": "^1.0.2",
    "path-to-regexp": "2.4.0",
    "prop-types": "^15.5.10",
    "qrcode.react": "^1.0.0",
    "qs": "^6.9.0",
    "react": "^16.8.6",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.6",
    "react-fittext": "^1.0.0",
    "react-helmet": "^5.2.1",
    "react-router": "^4.3.1",
    "react-to-print": "^2.9.0",
    "redux": "^4.0.1",
    "umi": "^3.0.7",
    "umi-plugin-antd-icon-config": "^2.0.0",
    "umi-request": "~1.2.19",
    "use-merge-value": "^1.0.2",
    "utility": "^1.16.3"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.14",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.144",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.13",
    "@umijs/fabric": "2.0.0",
    "@umijs/preset-react": "^1.2.2",
    "@umijs/preset-ui": "^2.1.9",
    "chalk": "^3.0.0",
    "check-prettier": "^1.0.3",
    "cross-env": "^6.0.0",
    "cross-port-killer": "^1.1.1",
    "enzyme": "^3.9.0",
    "express": "^4.17.1",
    "gh-pages": "^2.0.1",
    "husky": "^4.0.7",
    "import-sort-cli": "^6.0.0",
    "jest-puppeteer": "^4.2.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^9.0.0",
    "mockjs": "^1.0.1-beta3",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "stylelint": "^12.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0"
  },
  "optionalDependencies": {
    "puppeteer": "^2.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
